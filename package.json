{
  "name": "example-typescript-webapp",
  "version": "1.0.0",
  "description": "Example Web Application",
  "main": "index.js",
  "scripts": {
    "test": "eslint \"**/*.{ts,tsx}\"",
    "start": "NODE_ENV=development webpack serve --config webpack.dev.js",
    "build:watch": "NODE_ENV=development webpack -w --config webpack.dev.js",
    "build:run": "nodemon app.js",
    "build": "npm run clean; NODE_ENV=production webpack --color --config webpack.prod.js",
    "clean": "rimraf build",
    "dev": "npm run build:watch & npm run build:run",
    "docker:clean": "docker image prune",
    "docker:debug": "docker-compose -f docker-compose.debug.yml -p example_webapp_debug up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml -p example_webapp_prod up --build",
    "docker:test": "docker-compose -f docker-compose.test.yml -p example_webapp_test up --build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmonfre/example-typescript-webapp.git"
  },
  "author": "Thomas Monfre",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tmonfre/example-typescript-webapp/issues"
  },
  "homepage": "https://github.com/tmonfre/example-typescript-webapp#readme",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.15",
    "path": "^0.12.7",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "buffer": "^6.0.3",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0"
  }
}
